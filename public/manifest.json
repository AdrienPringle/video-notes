{
	"manifest_version": 2,
	"name": "video notes",
	"version": "0.1",
	"permissions": ["tabs", "activeTab", "declarativeContent", "storage"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["/static/css/main.css"],
			"js": ["/static/js/main.js"]
		}
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup/popup.html"
	}
}
